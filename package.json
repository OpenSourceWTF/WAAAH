{
  "name": "@opensourcewtf/waaah",
  "version": "0.1.0",
  "description": "Work Anywhere Autonomous Agent Hub - Multi-agent orchestration via MCP",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidtai/WAAAH.git"
  },
  "homepage": "https://github.com/davidtai/WAAAH#readme",
  "bugs": {
    "url": "https://github.com/davidtai/WAAAH/issues"
  },
  "keywords": [
    "waaah",
    "mcp",
    "ai-agents",
    "orchestration",
    "multi-agent"
  ],
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "serve": "tsx packages/mcp-server/src/index.ts",
    "serve:prod": "node packages/mcp-server/dist/index.js",
    "cli": "tsx packages/cli/src/index.ts",
    "bot": "tsx --watch packages/bot/src/index.ts",
    "bot:prod": "node packages/bot/dist/index.js",
    "proxy": "node packages/mcp-proxy/dist/index.js",
    "publish:all": "pnpm -r publish --access public",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart",
    "docker:reload": "docker compose down && docker compose build && docker compose up -d",
    "docker:ps": "docker compose ps",
    "docker:local:up": "docker compose -f docker-compose.local.yml up -d",
    "docker:local:down": "docker compose -f docker-compose.local.yml down",
    "docker:local:build": "docker compose -f docker-compose.local.yml build",
    "docker:local:logs": "docker compose -f docker-compose.local.yml logs -f",
    "docker:local:reload": "docker compose -f docker-compose.local.yml down && docker compose -f docker-compose.local.yml build && docker compose -f docker-compose.local.yml up -d"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}